{
  "name": "@clscripts/typeorm-ts-node-commonjs",
  "description": "typeorm-ts-node-commonjs clscripts implementation",
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "author": "Islam Yamor",
  "version": "0.3.20",
  "prettier": "@samislam/prettier-config",
  "keywords": [
    "typeorm-ts-node-commonjs",
    "typeorm-ts-node-commonjs-cli",
    "cl-typeorm-ts-node-commonjs",
