    "format": "prettier --write ./{src,tests}/**/*.ts "
  },
  "files": [
    "dist"
  ],
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@repo/typescript-config": "workspace:^",
    "@types/jest": "^30.0.0",
    "barrelsby": "^2.8.1",
    "del-cli": "^6.0.0",
    "eslint": "^9.30.1",
    "jest": "^30.0.4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.3.0",
    "typescript": "5.8.2",
    "typescript-eslint": "^8.35.1"
  },
  "dependencies": {
    "@clscripts/cl-common": "^1.0.4"
  }
