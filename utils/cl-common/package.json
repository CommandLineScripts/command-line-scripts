{
  "name": "@clscripts/cl-common",
  "version": "1.0.5",
  "description": "Utilities for running and building clscripts scripts",
  "main": "./dist/index.js",
  "type": "commonjs",
  "types": "./dist/index.d.ts",
  "prettier": "@samislam/prettier-config",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "dev": "ts-node-dev",
    "lint": "tspc -p ./tsconfig.build.json --noEmit && eslint . --ext .ts",
    "clean": "del dist node_modules .turbo",
    "format": "prettier --write ./{src,tests}/**/*.ts ",
    "build": "barrelsby --directory ./src --delete && tspc -p ./tsconfig.build.json"
  },
  "keywords": [],
  "author": "Islam Yamor",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/jest": "^30.0.0",
    "barrelsby": "^2.8.1",
    "eslint": "^9.30.1",
    "jest": "^30.0.4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.3.0",
    "typescript": "5.8.2",
    "typescript-eslint": "^8.35.1"
  }
}
